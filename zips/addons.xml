<addon id="plugin.program.JanSolowizardmod" name="[COLOR blue]JanSolo[/COLOR][COLOR white]Wizard[/COLOR]" version="1.1.5" provider-name="JanSolo">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.repository" name="JL Repository">
		<dir>
		<info compressed="false">https://raw.githubusercontent.com/walterbishop2019/jansolo/master/zips/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/walterbishop2019/jansolo/master/zips/addons.xml.md5</checksum>
